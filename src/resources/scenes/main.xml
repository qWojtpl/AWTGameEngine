filesScroll="pos{0;0}size{10;180}parent{filesPanel}ScrollComponent:ObjectComponent{}Canvas:ObjectComponent{}ScrollCameraBind:ObjectComponent{}";

@editor="pos{0;0}size{0;0}pl.AWTGameEngine.custom.editor.Editor:ObjectComponent-C{}";

